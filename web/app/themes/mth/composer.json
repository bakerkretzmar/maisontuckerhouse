{
    "name": "roots/sage",
    "type": "wordpress-theme",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require": {
        "php": "~8.0.0",
        "log1x/sage-directives": "^1.1"
    },
    "extra": {
        "acorn": {
            "providers": [
                "App\\Providers\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
